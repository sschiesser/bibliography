	Keywords = {AHMI, Adult, Aged, Aging, physiology, Analysis of Variance, Female, Fingers, Hand, Hand Strength, Humans, Male, Muscle, Muscle Contraction, Sex Characteristics, Skeletal, finger, force, measurement, pressing},
	Language = {en},
