	Issn = {0167-9457},
	Journal = {Human Movement Science},
	Keywords = {AHMI,Achilles Tendon,Achilles Tendon: physiology,Adult,Balance,Biomechanics,Cutaneous,Feedback,Female,Foot,Foot: physiology,Humans,Male,Muscle,Postural Balance,Postural Balance: physiology,Sensory,Sensory: physiology,Skeletal,Skeletal: physiology,Somatosensory integration,Spindles,Vibration,Weight-Bearing,Weight-Bearing: physiology},
