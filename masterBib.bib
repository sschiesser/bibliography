	File = {::},
	Keywords = {AHMI,DMI,devices,feedback,force,tactile,vibration},
	Mendeley-Tags = {AHMI,DMI,devices,feedback,force,tactile,vibration},
	Title = {{Cymatic. Restoring the Physical Manifestation of Digital Sound Using Haptic Interfaces to Control a New Computer Based Musical Instrument}},
	Year = {2002}}

