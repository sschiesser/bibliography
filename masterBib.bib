@inproceedings{Rimell2002,
	Abstract = {The trend towards the virtual in music synthesis has lead to the inevitable decline of the physical, effectively inserting a `veil of tactile paralysis' between the musician and the sound source. Cymatic is a new computer based musical instrument which attempts to reverse this tendency by allowing users to interact with virtual instruments via tactile and gestural interfaces. Utilising a mass/spring physical modelling paradigm, Cymatic models multi-dimensional, interconnectable resonating structures that can be played in real-time with various excitation methods. Gestural control as well as tactile and proprioceptive feedback is provided via a force feedback joystick and a tactile feedback mouse, restoring the musician's sense of working with a true physical instrument in the natural world.},
	Author = {Rimell, Stuart and Howard, David M and Tyrrell, Andy M and Kirk, Ross and Hunt, Andy},
	Booktitle = {International Computer Music Conference (ICMC)},
	Date-Added = {2015-06-02 09:50:31 +0000},
