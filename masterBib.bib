@article{Pfordresher2006,
	Abstract = {This review summarizes recent research on the way in which music performance may rely on the perception of sounds that accompany actions (termed auditory feedback). Alterations of auditory feedback can profoundly disrupt performance, though not all alterations cause dis- ruption and different alterations generate different types of disruption. Recent results have revealed a basic distinction between the role of feedback contents (musical pitch) and the degree to which feedback onsets are synchronized with actions. These results further suggest a theoretical framework for the coordination of actions with feedback in which perception and action share a common representation of sequence structure.},
