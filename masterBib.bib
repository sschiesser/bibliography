	Mendeley-Tags = {AHMI},
	Month = jun,
	Number = {3},
	Pages = {395--404},
	Pmid = {22441956},
	Title = {{Standard errors and confidence intervals in within-subjects designs: generalizing Loftus and Masson (1994) and avoiding the biases of alternative accounts.}},
