	Keywords = {AHMI,auditory,evaluation,experiment,haptic,perception,quality},
	Mendeley-Tags = {AHMI,auditory,evaluation,experiment,haptic,perception,quality},
	Number = {1/2},
	Pages = {38--46},
	Title = {{Quality of Auditory-Tactile Virtual Environments}},
	Volume = {60},
	Year = {2012}}

