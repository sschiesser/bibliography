	Mendeley-Tags = {AHMI,loudness,pitch,sound,touch,vibration},
	Month = jan,
	Number = {October},
	Pages = {160},
	Pmid = {21887147},
	Title = {{Separate mechanisms for audio-tactile pitch and loudness interactions.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3157934\&tool=pmcentrez\&rendertype=abstract},
	Volume = {1},
